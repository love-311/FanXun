        </activity>
        <activity android:name=".activity.MainActivity"></activity>
        <activity android:name=".activity.AddUsedHouseActivity"></activity>
        <activity android:name=".activity.OrientationActivity"></activity>
        <activity android:name=".activity.DecorationActivity"></activity>
        <activity android:name=".activity.OwnerRemarkActivity"></activity>
