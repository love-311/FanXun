        <activity android:name=".activity.TypePassengerActivity"></activity>
        <activity android:name=".activity.StatusPassengerActivity"></activity>
        <activity android:name=".activity.DecorationPassengerActivity"></activity>
        <activity android:name=".activity.OrientationPassengerActivity"></activity>
