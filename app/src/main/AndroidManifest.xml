        <activity android:name=".activity.GarageActivity"></activity>
        <activity android:name=".activity.KeyActivity"></activity>
        <activity android:name=".activity.HouseSourceRemarkActivity"></activity>
        <activity android:name=".activity.VillageActivity"></activity>
        <activity android:name=".activity.HouseTypeActivity"></activity>
        <activity android:name=".activity.PickPictureActivity"></activity>
        <activity android:name=".activity.DelegateActivity"></activity>
        <activity android:name=".activity.RentTimeActivity"></activity>
        <activity android:name=".activity.SearchActivity"></activity>
        <activity android:name=".activity.BrokerActivity"></activity>
        <activity android:name=".activity.PlateActivity"></activity>
        <activity android:name=".activity.HouseTypeSearchActivity"></activity>
        <activity android:name=".activity.AddPassengerActivity"></activity>
        <activity android:name=".activity.PassengerRemarkActivity"></activity>
        <activity android:name=".activity.PassengerPropertiesActivity"></activity>
        <activity android:name=".activity.SearchPassengerActivity"></activity>
        <activity android:name=".activity.UsedHouseDetailActivity"
