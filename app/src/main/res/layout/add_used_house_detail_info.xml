            android:layout_gravity="center_vertical"
            android:layout_marginLeft="24px"
            android:text="楼层："
            android:textColor="#9c9a9c"
            android:textSize="34px" />

        <EditText
            android:id="@+id/et_floor_numbers"
            android:layout_width="100px"
            android:layout_height="60px"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="16px"
            android:background="@drawable/et"
            android:inputType="number"
            android:maxLength="3"
            android:paddingLeft="10px"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="12px"
            android:text="层/共"
            android:textSize="34px" />
        <EditText
            android:id="@+id/et_total_floor_numbers"
            android:layout_width="100px"
            android:layout_height="60px"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="16px"
            android:background="@drawable/et"
            android:inputType="number"
            android:text="0"
            android:maxLength="3"
            android:paddingLeft="10px"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="12px"
            android:text="层"
            android:textSize="34px" />
       、
    </LinearLayout>

    <include layout="@layout/add_info_1px"></include>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="90px"
        android:background="#ffffff">

        <TextView
            android:id="@+id/house_number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="24px"
            android:text="房号："
            android:textColor="#9c9a9c"
            android:textSize="34px" />

        <EditText
            android:id="@+id/et_house_number_name"
            android:layout_width="480px"
            android:layout_height="60px"
            android:layout_centerVertical="true"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="16px"
            android:layout_toRightOf="@+id/house_number"
            android:background="@drawable/et"
            android:maxLength="8"
            android:paddingLeft="10px"/>
    </RelativeLayout>

    <include layout="@layout/add_info_1px"></include>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="90px"
        android:background="#ffffff"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="24px"
            android:text="产权："
            android:textColor="#9c9a9c"
            android:textSize="34px" />

        <LinearLayout
            android:id="@+id/ll_property_right"
            android:layout_width="0px"
